<model name="tutorial" namespace="http://www.intermine.org/model/tutorial#">
    <class name="org.intermine.model.tutorial.Broke" is-interface="true">
        <attribute name="debt" type="int"/>
        <reference name="bank" referenced-type="org.intermine.model.tutorial.Bank" reverse-reference="debtors"/>
    </class>

    <class name="org.intermine.model.tutorial.Thing" is-interface="true"></class>

    <class name="org.intermine.model.tutorial.Employable" extends="org.intermine.model.tutorial.Thing" is-interface="true">
        <attribute name="name" type="java.lang.String"/>
    </class>

    <class name="org.intermine.model.tutorial.HasAddress" is-interface="true">
        <reference name="address" referenced-type="org.intermine.model.tutorial.Address"/>
    </class>

    <class name="org.intermine.model.tutorial.HasSecretarys" is-interface="true">
        <collection name="secretarys" referenced-type="org.intermine.model.tutorial.Secretary" ordered="true"/>
    </class>

    <class name="org.intermine.model.tutorial.Contractor" extends="org.intermine.model.tutorial.Employable org.intermine.model.tutorial.ImportantPerson" is-interface="false">
        <reference name="personalAddress" referenced-type="org.intermine.model.tutorial.Address"/>
        <reference name="businessAddress" referenced-type="org.intermine.model.tutorial.Address"/>
        <collection name="companys" referenced-type="org.intermine.model.tutorial.Company" ordered="true" reverse-reference="contractors"/>
        <collection name="oldComs" referenced-type="org.intermine.model.tutorial.Company" ordered="true" reverse-reference="oldContracts"/>
    </class>

    <class name="org.intermine.model.tutorial.Manager" extends="org.intermine.model.tutorial.Employee org.intermine.model.tutorial.ImportantPerson" is-interface="false">
        <attribute name="title" type="java.lang.String"/>
    </class>

    <class name="org.intermine.model.tutorial.Employee" extends="org.intermine.model.tutorial.Employable org.intermine.model.tutorial.HasAddress" is-interface="false">
        <attribute name="fullTime" type="boolean"/>
        <attribute name="age" type="int"/>
        <reference name="department" referenced-type="org.intermine.model.tutorial.Department" reverse-reference="employees"/>
        <reference name="departmentThatRejectedMe" referenced-type="org.intermine.model.tutorial.Department" reverse-reference="rejectedEmployee"/>
    </class>

    <class name="org.intermine.model.tutorial.Department" extends="org.intermine.model.tutorial.RandomInterface" is-interface="false">
        <attribute name="name" type="java.lang.String"/>
        <reference name="company" referenced-type="org.intermine.model.tutorial.Company" reverse-reference="departments"/>
        <reference name="manager" referenced-type="org.intermine.model.tutorial.Manager"/>
        <collection name="employees" referenced-type="org.intermine.model.tutorial.Employee" ordered="true" reverse-reference="department"/>
        <collection name="rejectedEmployee" referenced-type="org.intermine.model.tutorial.Employee" ordered="true" reverse-reference="departmentThatRejectedMe"/>
    </class>

    <class name="org.intermine.model.tutorial.Company" extends="org.intermine.model.tutorial.RandomInterface org.intermine.model.tutorial.HasAddress org.intermine.model.tutorial.HasSecretarys" is-interface="true">
        <attribute name="name" type="java.lang.String"/>
        <attribute name="vatNumber" type="int"/>
        <reference name="CEO" referenced-type="org.intermine.model.tutorial.CEO" reverse-reference="company"/>
        <collection name="departments" referenced-type="org.intermine.model.tutorial.Department" ordered="true" reverse-reference="company"/>
        <collection name="contractors" referenced-type="org.intermine.model.tutorial.Contractor" ordered="true" reverse-reference="companys"/>
        <collection name="oldContracts" referenced-type="org.intermine.model.tutorial.Contractor" ordered="true" reverse-reference="oldComs"/>
    </class>

    <class name="org.intermine.model.tutorial.Address" extends="org.intermine.model.tutorial.Thing" is-interface="false">
        <attribute name="address" type="java.lang.String"/>
    </class>

    <class name="org.intermine.model.tutorial.RandomInterface" is-interface="true">
    </class>

    <class name="org.intermine.model.tutorial.CEO" extends="org.intermine.model.tutorial.Manager org.intermine.model.tutorial.HasSecretarys" is-interface="false">
        <attribute name="salary" type="int"/>
        <reference name="company" referenced-type="org.intermine.model.tutorial.Company" reverse-reference="CEO"/>
    </class>

    <class name="org.intermine.model.tutorial.ImportantPerson" is-interface="true">
        <attribute name="seniority" type="java.lang.Integer"/>
    </class>

    <class name="org.intermine.model.tutorial.Secretary" is-interface="false">
        <attribute name="name" type="java.lang.String"/>
    </class>

    <class name="org.intermine.model.tutorial.Bank" is-interface="false">
        <attribute name="name" type="java.lang.String"/>
        <collection name="debtors" referenced-type="org.intermine.model.tutorial.Broke" ordered="false" reverse-reference="bank"/>
    </class>
</model>
